		<item value = "4" text = "梦幻荧光" />
	</Selector>


	<Selector className = "BooleanSelector" valueType = "int" width = "80" export="value=u8">
		<item value = "0" text = "否" />
		<item value = "1" text = "是" />
	</Selector>
	<Selector className = "Boolean3Selector" valueType = "int" width = "80" export="value=u8">
